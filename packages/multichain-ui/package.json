{
  "name": "@metamask/multichain-ui",
  "version": "0.0.0",
  "description": "MetaMask Connect Install Modal for Web",
  "keywords": [
    "MetaMask",
    "Ethereum"
  ],
  "homepage": "https://github.com/MetaMask/metamask-connect-monorepo/tree/main/packages/multichain-ui#readme",
  "bugs": {
    "url": "https://github.com/MetaMask/metamask-connect-monorepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/metamask-connect-monorepo.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./loader": {
      "import": "./dist/loader/index.js",
      "require": "./dist/loader/index.cjs.js"
    },
    "./loader/*": "./dist/loader/*",
    "./dist/loader/*": "./dist/loader/*",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "collection": "dist/collection/collection-manifest.json",
  "scripts": {
    "build:docs": "typedoc",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --outDir dist/types",
    "build:clean": "yarn clean && yarn build",
    "build": "ts-bridge --project tsconfig.build.json --verbose --clean --no-references",
    "build:dev": "NODE_ENV=development stencil build --debug --dev",
    "build:tsc": "tsc --build tsconfig.json --verbose",
    "build:watch": "stencil build --watchAll",
    "changelog:update": "../../scripts/update-changelog.sh @metamask/multichain-ui",
    "changelog:validate": "../../scripts/validate-changelog.sh @metamask/multichain-ui",
    "dev": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "prepare-manifest:preview": "../../scripts/prepare-preview-manifest.sh",
    "publish:preview": "yarn npm publish --tag preview",
    "since-latest-release": "../../scripts/since-latest-release.sh",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "test:ci": "jest --coverage --passWithNoTests --setupFilesAfterEnv ./jest-preload.js",
    "preversion": "yarn test",
    "postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "dependencies": {
    "@paulmillr/qr": "^0.2.1",
    "qr-code-styling": "^1.9.2"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.3.1",
    "@metamask/auto-changelog": "^3.4.3",
    "@size-limit/preset-big-lib": "^11.0.2",
    "@stencil/core": "4.22.2",
    "@types/i18n": "^0.13.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/prettier": "^2",
    "deepmerge": "^4.2.2",
    "jest": "^29.6.4",
    "puppeteer": "^23.8.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "size-limit": "^11.1.6",
    "typedoc": "^0.23.22",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "~5.2.2"
  },
  "engines": {
    "node": "^18.18 || >=20"
  },
  "lavamoat": {
    "allowScripts": {
      "webpack-dev-server>ws>bufferutil": false,
      "webpack-dev-server>ws>utf-8-validate": false
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
